<h1>Information for Doctor <%= @doctor.name %></h1>
<h3>Alma Mater:</h3>
<%= @doctor.university %>

<h3>Specialty:</h3>
<%= @doctor.specialty %>

<h3>This Doctor works for:</h3>
<%= @doctor.hospital.name %>

<h3>Patient List:</h3>
<ul>
  <% @doctor.patients.each do |patient| %>
    <li id="patient_<%= patient.id %>"><%= patient.name %>
    <%= button_to 'Remove Patient', doctor_patient_path(), method: :delete %>
    </li>
  <% end %>
</ul>